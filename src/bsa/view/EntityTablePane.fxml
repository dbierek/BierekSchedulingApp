         <children>
            <Button fx:id="addButton" layoutX="14.0" layoutY="310.0" text="Add" prefHeight="30.0" />
            <Button fx:id="editButton" layoutX="92.0" layoutY="310.0" text="Edit" prefHeight="30.0" />
